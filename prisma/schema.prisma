generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Url {
  id         Int      @id @default(autoincrement())
  shortCode  String?  @unique
  longUrl    String
  createdAt  DateTime @default(now())
  clickCount Int      @default(0)
  expiresAt  DateTime? @db.Timestamp(6)
  ownerId    Int? 
}
